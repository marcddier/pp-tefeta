    "prettier-eslint": "^9.0.1",
    "serialize-javascript": ">=3.0.0",
    "ts-jest": "^25.5.1",
    "ts-node": "8.10.1",
    "tsconfig-paths": "^3.9.0",
    "shx": "^0.3.2",
    "pretty-quick": "^2.0.1",
    "typescript": "3.8.3"
  }
